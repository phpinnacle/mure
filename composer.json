{
    "name": "phpinnacle/mure",
    "type": "library",
    "description": "PHPinnacle Z-Engine extensions",
    "keywords": [
        "phpinnacle", "zengine", "ffi"
    ],
    "homepage": "https://github.com/phpinnacle/mure",
    "license": "MIT",
    "authors": [
        {
            "name": "PHPinnacle",
            "email": "dev@phpinnacle.com",
            "homepage": "https://phpinnacle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "lisachenko/z-engine": "^0.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PHPinnacle\\Mure\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPinnacle\\Mure\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
